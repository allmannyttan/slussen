{
  "name": "api",
  "version": "1.0.1",
  "description": "Integration med fastAPI.",
  "main": "./index.js",
  "scripts": {
    "build": "tsc",
    "dev": "node-pg-migrate up && ts-node-dev --no-notify --ignore-watch node_modules -r tsconfig-paths/register src/server.ts",
    "docs": "node generateSwagger",
    "lint": "eslint ./src/**/*.ts",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate": "node-pg-migrate -f migrate-config.json",
    "prod": "tsc && node ./dist/lib/server.js",
    "start": "node-pg-migrate up && ts-node --files -r tsconfig-paths/register src/server.ts",
    "test:ci": "jest --ci --forceExit",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test": "npm run test:watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Allmannyttans-Digitaliseringsinitiativ/api.git"
  },
  "author": "Anders, Martin",
  "license": "ISC",
  "dependencies": {
    "@iteam/config": "^12.1.2",
    "axios": "0.21.2",
    "body-parser": "1.19.0",
    "connect-timeout": "^1.9.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-async-handler": "1.1.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "node-pg-migrate": "4.6.2",
    "openapi-types": "^7.2.3",
    "pg": "8.0.3",
    "sql-template-strings": "^2.2.2",
    "typescript": "^4.4.4",
    "xml2js": "0.4.23",
    "xml2json": "0.12.0"
  },
  "devDependencies": {
    "@iteam/eslint-config-node": "github:iteam1337/eslint-config-node#dfde1d0",
    "@types/connect-timeout": "0.0.35",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.4",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/pg": "7.14.3",
    "@types/supertest": "^2.0.10",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/xml2js": "0.4.5",
    "@types/xml2json": "0.11.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "dotenv": "8.2.0",
    "eslint": "^8.1.0",
    "jest": "^27.3.0",
    "jest-watch-typeahead": "0.5.0",
    "prettier": "2.0.4",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^6.0.6",
    "ts-jest": "^27.0.0",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.1",
    "ts-to-json": "^0.65.4",
    "tsconfig-paths": "3.9.0"
  },
  "keywords": []
}
